<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="610.0" maxWidth="1222.0" minHeight="610.0" minWidth="1222.0" prefHeight="610.0" prefWidth="1222.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.concordia.UIController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label fx:id="title" text="Guest User">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" />
      <Button mnemonicParsing="false" prefWidth="114.0" text="Guest" onAction="#onGuestButton">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" text="Registered" onAction="#onRegistedButton">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" prefHeight="37.0" prefWidth="114.0" text="Airport" onAction="#onAirportButton">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
   </children>
</VBox>
